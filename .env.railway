# Bot settings
# TELEGRAM_BOT_TOKEN should be set in Railway environment variables
# BOT_USERNAME should be set in Railway environment variables

# Webhook settings (for production)
USE_WEBHOOK=true
# WEBHOOK_URL should be set in Railway environment variables
WEBHOOK_PATH=/telegram/webhook
PORT=8443

# API Keys
# OPENAI_API_KEY should be set in Railway environment variables
# DEEPSEEK_API_KEY should be set in Railway environment variables
# STEEL_API_KEY should be set in Railway environment variables
# OPENROUTER_API_KEY should be set in Railway environment variables

# Browser settings
BROWSER_HEADLESS=true
BROWSER_BROWSERLESS=true
# BROWSERLESS_URL should be set in Railway environment variables

# AI Model settings
GPT_MODEL=gpt-4o
DEEPSEEK_MODEL=deepseek-reasoner
CLAUDE_MODEL=anthropic/claude-3-7-sonnet-20250219

# Timeouts and limits
SEARCH_TIMEOUT=90
MAX_RETRIES=3
MESSAGE_CHUNK_SIZE=4000

# Storage settings
MAX_HISTORY_LENGTH=10

# Supabase settings
# SUPABASE_URL should be set in Railway environment variables
# SUPABASE_KEY should be set in Railway environment variables

# Railway specific settings
RAILWAY_ENVIRONMENT=production
DISABLE_GIF_CREATION=true 